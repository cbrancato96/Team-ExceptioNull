#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x12cf430 .scope module, "test" "test" 2 38;
 .timescale 0 0;
v0x1307bc0_0 .var "clk", 0 0;
v0x1307c60_0 .var "jump_offset", 7 0;
v0x1307d10_0 .net "offset", 7 0, L_0x1307800; 1 drivers
v0x1307dc0_0 .net "pc", 7 0, v0x1307880_0; 1 drivers
v0x1307ea0_0 .var "pc_control", 7 0;
v0x1307f50_0 .net "pc_update", 7 0, L_0x13086b0; 1 drivers
S_0x1307ad0 .scope task, "display" "display" 2 83, 2 83, S_0x12cf430;
 .timescale 0 0;
TD_test.display ;
    %delay 1, 0;
    %vpi_call 2 84 "$display", "pc:%b, pc_update=%b, pc_control:%b, jump_offset=%b,, offset=%b, clk=%b", v0x1307dc0_0, v0x1307f50_0, v0x1307ea0_0, v0x1307c60_0, v0x1307d10_0, v0x1307bc0_0;
    %end;
S_0x12bd690 .scope module, "PROGRAM_COUNTER" "program_counter" 2 47, 2 1, S_0x12cf430;
 .timescale 0 0;
L_0x1307800 .functor AND 8, v0x1307ea0_0, v0x1307c60_0, C4<11111111>, C4<11111111>;
v0x12d19e0_0 .net *"_s10", 31 0, L_0x1308440; 1 drivers
v0x13071e0_0 .net *"_s13", 23 0, C4<000000000000000000000000>; 1 drivers
v0x1307280_0 .net *"_s14", 31 0, L_0x1308570; 1 drivers
v0x1307320_0 .net *"_s2", 31 0, L_0x1308070; 1 drivers
v0x13073d0_0 .net *"_s5", 23 0, C4<000000000000000000000000>; 1 drivers
v0x1307470_0 .net *"_s6", 31 0, C4<00000000000000000000000000000001>; 1 drivers
v0x1307550_0 .net *"_s8", 31 0, L_0x1308230; 1 drivers
v0x13075f0_0 .net "clk", 0 0, v0x1307bc0_0; 1 drivers
v0x13076e0_0 .net "jump_offset", 7 0, v0x1307c60_0; 1 drivers
v0x1307780_0 .alias "offset", 7 0, v0x1307d10_0;
v0x1307880_0 .var "pc", 7 0;
v0x1307920_0 .net "pc_control", 7 0, v0x1307ea0_0; 1 drivers
v0x1307a30_0 .alias "pc_update", 7 0, v0x1307f50_0;
E_0x12bc310 .event posedge, v0x13075f0_0;
L_0x1308070 .concat [ 8 24 0 0], v0x1307880_0, C4<000000000000000000000000>;
L_0x1308230 .arith/sum 32, L_0x1308070, C4<00000000000000000000000000000001>;
L_0x1308440 .concat [ 8 24 0 0], L_0x1307800, C4<000000000000000000000000>;
L_0x1308570 .arith/sum 32, L_0x1308230, L_0x1308440;
L_0x13086b0 .part L_0x1308570, 0, 8;
    .scope S_0x12bd690;
T_1 ;
    %set/v v0x1307880_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_0x12bd690;
T_2 ;
    %wait E_0x12bc310;
    %load/v 8, v0x13075f0_0, 1;
    %jmp/0xz  T_2.0, 8;
    %load/v 8, v0x1307a30_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v0x1307880_0, 0, 8;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x12cf430;
T_3 ;
    %vpi_call 2 57 "$display", "Initial conditions";
    %set/v v0x1307bc0_0, 0, 1;
    %set/v v0x1307ea0_0, 0, 8;
    %movi 8, 3, 8;
    %set/v v0x1307c60_0, 8, 8;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %set/v v0x1307bc0_0, 1, 1;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %set/v v0x1307bc0_0, 0, 1;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %set/v v0x1307ea0_0, 1, 8;
    %set/v v0x1307bc0_0, 1, 1;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %set/v v0x1307ea0_0, 0, 8;
    %set/v v0x1307bc0_0, 0, 1;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %set/v v0x1307bc0_0, 1, 1;
    %fork TD_test.display, S_0x1307ad0;
    %join;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 3;
