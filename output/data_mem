#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-17-g9be3fc3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x2700560 .scope module, "memory" "memory" 2 26;
 .timescale 0 0;
v0x2701520 .array "data_memory", 0 63, 7 0;
S_0x27013a0 .scope module, "test" "test" 2 35;
 .timescale 0 0;
v0x2721600_0 .var "data_address", 5 0;
v0x27216e0_0 .net "read_data", 7 0, L_0x2721a70;  1 drivers
v0x27217b0_0 .var "write_data", 7 0;
v0x27218b0_0 .var "write_enable", 0 0;
S_0x26fa6d0 .scope module, "data_mem" "data_mem" 2 42, 2 1 0, S_0x27013a0;
 .timescale 0 0;
    .port_info 0 /INPUT 6 "data_address"
    .port_info 1 /INPUT 8 "write_data"
    .port_info 2 /INPUT 1 "write_enable"
    .port_info 3 /OUTPUT 8 "read_data"
L_0x2721a70 .functor BUFZ 8, L_0x2721980, C4<00000000>, C4<00000000>, C4<00000000>;
v0x26fa8c0_0 .net *"_s0", 7 0, L_0x2721980;  1 drivers
v0x2721100_0 .net "data_address", 5 0, v0x2721600_0;  1 drivers
v0x27211e0 .array "mem", 0 4, 7 0;
v0x27212b0_0 .net "read_data", 7 0, L_0x2721a70;  alias, 1 drivers
v0x2721390_0 .net "write_data", 7 0, v0x27217b0_0;  1 drivers
v0x27214c0_0 .net "write_enable", 0 0, v0x27218b0_0;  1 drivers
E_0x26fbaa0 .event edge, v0x27214c0_0;
L_0x2721980 .array/port v0x27211e0, v0x2721600_0;
    .scope S_0x2700560;
T_0 ;
    %vpi_call 2 30 "$readmemh", "datamemory.list", v0x2701520 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x26fa6d0;
T_1 ;
    %wait E_0x26fbaa0;
    %load/vec4 v0x27214c0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x2721390_0;
    %ix/getv 3, v0x2721100_0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x27211e0, 0, 4;
T_1.0 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x27013a0;
T_2 ;
    %vpi_call 2 49 "$display", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x2721600_0, v0x27217b0_0, v0x27218b0_0, v0x27216e0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x27218b0_0, 0, 1;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x2721600_0, 0, 6;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x27217b0_0, 0, 8;
    %vpi_call 2 53 "$monitor", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x2721600_0, v0x27217b0_0, v0x27218b0_0, v0x27216e0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "/home/krishna/Desktop/CompArch/Team-ExceptioNull/data_mem.v";
