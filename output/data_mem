    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18e11f0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x18e0f50_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x18e1120_0, 0, 8;
