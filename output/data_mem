    .port_info 0 /INPUT 8 "data_address"
    .port_info 1 /INPUT 8 "write_data"
    .port_info 2 /INPUT 1 "write_enable"
    .port_info 3 /OUTPUT 8 "read_data"
    .port_info 4 /INPUT 1 "clk"
P_0x7fc0c3c077c0 .param/str "DATA" 0 2 18, "dataMem.bin";
v0x7fc0c3c07920_0 .net "clk", 0 0, v0x7fc0c3c0fc20_0;  1 drivers
v0x7fc0c3c0f820_0 .net "data_address", 7 0, v0x7fc0c3c0fcd0_0;  1 drivers
v0x7fc0c3c0f8c0 .array "data_mem", 127 64, 7 0;
v0x7fc0c3c0f970_0 .var "read_data", 7 0;
v0x7fc0c3c0fa10_0 .net "write_data", 7 0, v0x7fc0c3c0fe30_0;  1 drivers
v0x7fc0c3c0fb00_0 .net "write_enable", 0 0, v0x7fc0c3c0fee0_0;  1 drivers
E_0x7fc0c3c078f0 .event posedge, v0x7fc0c3c07920_0;
    .scope S_0x7fc0c3c07660;
T_0 ;
    %wait E_0x7fc0c3c078f0;
    %load/vec4 v0x7fc0c3c0fb00_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x7fc0c3c0fa10_0;
    %load/vec4 v0x7fc0c3c0f820_0;
    %subi 64, 0, 8;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x7fc0c3c0f8c0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x7fc0c3c0f820_0;
    %subi 64, 0, 8;
    %ix/vec4 4;
    %load/vec4a v0x7fc0c3c0f8c0, 4;
    %assign/vec4 v0x7fc0c3c0f970_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fc0c3c07660;
T_1 ;
    %vpi_call 2 31 "$readmemb", P_0x7fc0c3c077c0, v0x7fc0c3c0f8c0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x7fc0c3c07500;
T_2 ;
    %vpi_call 2 53 "$display", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x7fc0c3c0fcd0_0, v0x7fc0c3c0fe30_0, v0x7fc0c3c0fee0_0, v0x7fc0c3c0fd60_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc0c3c0fc20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc0c3c0fee0_0, 0, 1;
    %pushi/vec4 66, 0, 8;
    %store/vec4 v0x7fc0c3c0fcd0_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x7fc0c3c0fe30_0, 0, 8;
    %vpi_call 2 58 "$monitor", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b, clk=%b", v0x7fc0c3c0fcd0_0, v0x7fc0c3c0fe30_0, v0x7fc0c3c0fee0_0, v0x7fc0c3c0fd60_0, v0x7fc0c3c0fc20_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc0c3c0fee0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc0c3c0fc20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fc0c3c0fee0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fc0c3c0fc20_0, 0, 1;
