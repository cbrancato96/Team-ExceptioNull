v0x18e0d30_0 .net "write_enable", 0 0, v0x18e11f0_0;  1 drivers
E_0x18b9290 .event edge, L_0x18f1440;
L_0x18e1290 .concat [ 1 31 0 0], v0x18e0e90_0, L_0x7f7bbaefc018;
L_0x18f1440 .cmp/eq 32, L_0x18e1290, L_0x7f7bbaefc060;
    .scope S_0x18bff30;
T_0 ;
    %wait E_0x18b9290;
    %load/vec4 v0x18e0d30_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0x18e0bc0_0;
    %load/vec4 v0x18e0940_0;
    %subi 64, 0, 8;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x18e0a20, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x18e0d30_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x18e0940_0;
    %subi 64, 0, 8;
    %ix/vec4 4;
    %load/vec4a v0x18e0a20, 4;
    %assign/vec4 v0x18e0ae0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x18bff30;
T_1 ;
    %vpi_call 2 34 "$readmemb", P_0x18b83a0, v0x18e0a20 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x18bfdb0;
T_2 ;
