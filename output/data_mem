#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-17-g9be3fc3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1403e20 .scope module, "test" "test" 2 36;
 .timescale 0 0;
v0x1424b10_0 .var "clk", 0 0;
v0x1424bd0_0 .var "data_address", 7 0;
v0x1424ca0_0 .net "read_data", 7 0, v0x14247a0_0;  1 drivers
v0x1424da0_0 .var "write_data", 7 0;
v0x1424e70_0 .var "write_enable", 0 0;
S_0x1403fa0 .scope module, "DATA_MEMORY" "data_memory" 2 44, 2 1 0, S_0x1403e20;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "data_address"
    .port_info 1 /INPUT 8 "write_data"
    .port_info 2 /INPUT 1 "write_enable"
    .port_info 3 /OUTPUT 8 "read_data"
    .port_info 4 /INPUT 1 "clk"
P_0x13fd2b0 .param/str "DATA" 0 2 17, "dataMem.bin";
v0x13fd3d0_0 .net "clk", 0 0, v0x1424b10_0;  1 drivers
v0x14245f0_0 .net "data_address", 7 0, v0x1424bd0_0;  1 drivers
v0x14246d0 .array "data_mem", 127 64, 7 0;
v0x14247a0_0 .var "read_data", 7 0;
v0x1424880_0 .net "write_data", 7 0, v0x1424da0_0;  1 drivers
v0x14249b0_0 .net "write_enable", 0 0, v0x1424e70_0;  1 drivers
E_0x13fe1b0 .event posedge, v0x13fd3d0_0;
    .scope S_0x1403fa0;
T_0 ;
    %vpi_call 2 20 "$readmemb", P_0x13fd2b0, v0x14246d0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x1403fa0;
T_1 ;
    %wait E_0x13fe1b0;
    %load/vec4 v0x14249b0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %load/vec4 v0x1424880_0;
    %load/vec4 v0x14245f0_0;
    %subi 64, 0, 8;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x14246d0, 0, 4;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x14249b0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.2, 4;
    %load/vec4 v0x14245f0_0;
    %subi 64, 0, 8;
    %ix/vec4 4;
    %load/vec4a v0x14246d0, 4;
    %assign/vec4 v0x14247a0_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1403e20;
T_2 ;
    %vpi_call 2 52 "$display", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x1424bd0_0, v0x1424da0_0, v0x1424e70_0, v0x1424ca0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1424b10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1424e70_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x1424bd0_0, 0, 8;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x1424da0_0, 0, 8;
    %vpi_call 2 57 "$monitor", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b, clk=%b", v0x1424bd0_0, v0x1424da0_0, v0x1424e70_0, v0x1424ca0_0, v0x1424b10_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1424e70_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1424b10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1424e70_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1424b10_0, 0, 1;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "/home/krishna/Desktop/CompArch/Team-ExceptioNull/data_mem.v";
