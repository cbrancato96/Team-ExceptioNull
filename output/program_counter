#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-17-g9be3fc3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x14fdc90 .scope module, "program_counter" "program_counter" 2 1;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 8 "pc_control"
    .port_info 2 /INPUT 8 "jump_offset"
    .port_info 3 /OUTPUT 8 "pc"
o0x7f88a9e19138 .functor BUFZ 8, C4<zzzzzzzz>; HiZ drive
o0x7f88a9e190d8 .functor BUFZ 8, C4<zzzzzzzz>; HiZ drive
L_0x151f100 .functor AND 8, o0x7f88a9e19138, o0x7f88a9e190d8, C4<11111111>, C4<11111111>;
L_0x7f88a9dd0018 .functor BUFT 1, C4<00000001>, C4<0>, C4<0>, C4<0>;
v0x14c5d20_0 .net/2u *"_s0", 7 0, L_0x7f88a9dd0018;  1 drivers
v0x151e980_0 .net *"_s2", 7 0, L_0x151f060;  1 drivers
v0x151ea60_0 .net *"_s4", 7 0, L_0x151f100;  1 drivers
o0x7f88a9e190a8 .functor BUFZ 1, C4<z>; HiZ drive
v0x151eb50_0 .net "clk", 0 0, o0x7f88a9e190a8;  0 drivers
v0x151ec10_0 .net "jump_offset", 7 0, o0x7f88a9e190d8;  0 drivers
v0x151ed40_0 .var "pc", 7 0;
v0x151ee20_0 .net "pc_control", 7 0, o0x7f88a9e19138;  0 drivers
v0x151ef00_0 .net "pc_update", 7 0, L_0x151f280;  1 drivers
E_0x14c6450 .event posedge, v0x151eb50_0;
L_0x151f060 .arith/sum 8, v0x151ed40_0, L_0x7f88a9dd0018;
L_0x151f280 .arith/sum 8, L_0x151f060, L_0x151f100;
    .scope S_0x14fdc90;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x151ed40_0, 0, 8;
    %end;
    .thread T_0;
    .scope S_0x14fdc90;
T_1 ;
    %wait E_0x14c6450;
    %load/vec4 v0x151eb50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x151ef00_0;
    %assign/vec4 v0x151ed40_0, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
