#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-17-g9be3fc3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x10d8750 .scope module, "test" "test" 2 31;
 .timescale 0 0;
v0x10fa9e0_0 .var "clk", 0 0;
v0x10faa80_0 .var "jump_offset", 7 0;
v0x10fab50_0 .net "pc", 7 0, v0x10fa4d0_0;  1 drivers
v0x10fac50_0 .var "pc_control", 7 0;
S_0x10d2cb0 .scope module, "PROGRAM_COUNTER" "program_counter" 2 38, 2 1 0, S_0x10d8750;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 8 "pc_control"
    .port_info 2 /INPUT 8 "jump_offset"
    .port_info 3 /OUTPUT 8 "pc"
L_0x10fadf0 .functor AND 8, v0x10fac50_0, v0x10faa80_0, C4<11111111>, C4<11111111>;
L_0x7f81cab92018 .functor BUFT 1, C4<00000001>, C4<0>, C4<0>, C4<0>;
v0x10d2f10_0 .net/2u *"_s0", 7 0, L_0x7f81cab92018;  1 drivers
v0x10fa110_0 .net *"_s2", 7 0, L_0x10fad50;  1 drivers
v0x10fa1f0_0 .net *"_s4", 7 0, L_0x10fadf0;  1 drivers
v0x10fa2e0_0 .net "clk", 0 0, v0x10fa9e0_0;  1 drivers
v0x10fa3a0_0 .net "jump_offset", 7 0, v0x10faa80_0;  1 drivers
v0x10fa4d0_0 .var "pc", 7 0;
v0x10fa5b0_0 .net "pc_control", 7 0, v0x10fac50_0;  1 drivers
v0x10fa690_0 .net "pc_update", 7 0, L_0x10fafd0;  1 drivers
E_0x10d2210 .event posedge, v0x10fa2e0_0;
L_0x10fad50 .arith/sum 8, v0x10fa4d0_0, L_0x7f81cab92018;
L_0x10fafd0 .arith/sum 8, L_0x10fad50, L_0x10fadf0;
S_0x10fa7f0 .scope task, "display" "display" 2 71, 2 71 0, S_0x10d8750;
 .timescale 0 0;
TD_test.display ;
    %delay 1, 0;
    %vpi_call 2 72 "$display", "pc:%b, pc_control:%b, jump_offset=%b, clk=%b", v0x10fab50_0, v0x10fac50_0, v0x10faa80_0, v0x10fa9e0_0 {0 0 0};
    %end;
    .scope S_0x10d2cb0;
T_1 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x10fa4d0_0, 0, 8;
    %end;
    .thread T_1;
    .scope S_0x10d2cb0;
T_2 ;
    %wait E_0x10d2210;
    %load/vec4 v0x10fa2e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %load/vec4 v0x10fa690_0;
    %assign/vec4 v0x10fa4d0_0, 0;
T_2.0 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x10d8750;
T_3 ;
    %vpi_call 2 45 "$display", "Initial conditions" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x10fac50_0, 0, 8;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x10faa80_0, 0, 8;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x10fac50_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x10fac50_0, 0, 8;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x10fa9e0_0, 0, 1;
    %fork TD_test.display, S_0x10fa7f0;
    %join;
    %end;
    .thread T_3;
