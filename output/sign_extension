#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-17-g9be3fc3)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x18919f0 .scope module, "test" "test" 2 16;
 .timescale 0 0;
v0x18b5c40_0 .var "clk", 0 0;
v0x18b5d10_0 .var "immediate", 1 0;
v0x18b5de0_0 .net "sign_extended", 7 0, v0x18b5900_0;  1 drivers
S_0x1891b70 .scope module, "SIGN_EXTENSION" "sign_extension" 2 21, 2 1 0, S_0x18919f0;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "immediate"
    .port_info 1 /OUTPUT 8 "sign_extended"
    .port_info 2 /INPUT 1 "clk"
v0x185bd70_0 .net "clk", 0 0, v0x18b5c40_0;  1 drivers
v0x18b5820_0 .net "immediate", 1 0, v0x18b5d10_0;  1 drivers
v0x18b5900_0 .var "sign_extended", 7 0;
E_0x18923f0 .event posedge, v0x185bd70_0;
S_0x18b5a70 .scope task, "display" "display" 2 35, 2 35 0, S_0x18919f0;
 .timescale 0 0;
TD_test.display ;
    %delay 1, 0;
    %vpi_call 2 36 "$display", "immediate=%b, sign_extended=%b, clk=%b", v0x18b5d10_0, v0x18b5de0_0, v0x18b5c40_0 {0 0 0};
    %end;
    .scope S_0x1891b70;
T_1 ;
    %wait E_0x18923f0;
    %load/vec4 v0x18b5820_0;
    %pad/s 8;
    %assign/vec4 v0x18b5900_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x18919f0;
T_2 ;
    %vpi_call 2 27 "$display", "Initial conditions" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x18b5c40_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x18b5d10_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x18b5c40_0, 0, 1;
    %fork TD_test.display, S_0x18b5a70;
    %join;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
