S_0xc91ad0 .scope module, "test" "test" 2 24;
 .timescale 0 0;
v0xcb5580_0 .var "instruction_address", 7 0;
v0xcb5670_0 .net "instruction_data", 7 0, L_0xcb5830;  1 drivers
S_0xc91c50 .scope module, "INSTRUCTION_MEM" "instruction_mem" 2 28, 2 1 0, S_0xc91ad0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "instruction_address"
    .port_info 1 /OUTPUT 8 "instruction_data"
P_0xc91e20 .param/str "INSTRUCTIONS" 0 2 10, "bin.mem";
L_0xcb5830 .functor BUFZ 8, L_0xcb5740, C4<00000000>, C4<00000000>, C4<00000000>;
v0xc93cb0_0 .net *"_s0", 7 0, L_0xcb5740;  1 drivers
v0xcb5290_0 .net "instruction_address", 7 0, v0xcb5580_0;  1 drivers
v0xcb5370_0 .net "instruction_data", 7 0, L_0xcb5830;  alias, 1 drivers
v0xcb5460 .array "instruction_mem", 63 0, 7 0;
L_0xcb5740 .array/port v0xcb5460, v0xcb5580_0;
    .scope S_0xc91c50;
T_0 ;
    %vpi_call 2 15 "$readmemb", P_0xc91e20, v0xcb5460 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0xc91ad0;
T_1 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0xcb5580_0, 0, 8;
    %vpi_call 2 34 "$display", "instruction_address=%b,instruction_data=%b", v0xcb5580_0, v0xcb5670_0 {0 0 0};
    %vpi_call 2 36 "$monitor", "instruction_address=%b,instruction_data=%b", v0xcb5580_0, v0xcb5670_0 {0 0 0};
    %end;
    .thread T_1;
