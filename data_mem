#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x813690 .scope module, "memory" "memory" 2 26;
 .timescale 0 0;
v0x813870 .array "data_memory", 5 0, 7 0;
S_0x813780 .scope module, "test" "test" 2 35;
 .timescale 0 0;
v0x85a570_0 .var "data_address", 5 0;
v0x85a610_0 .net "read_data", 7 0, L_0x85a940; 1 drivers
v0x85a6c0_0 .var "write_data", 7 0;
v0x85a770_0 .var "write_enable", 0 0;
S_0x849650 .scope module, "data_mem" "data_mem" 2 42, 2 1, S_0x813780;
 .timescale 0 0;
L_0x85a940 .functor BUFZ 8, L_0x85a850, C4<00000000>, C4<00000000>, C4<00000000>;
v0x849740_0 .net *"_s0", 7 0, L_0x85a850; 1 drivers
v0x85a220_0 .net "data_address", 5 0, v0x85a570_0; 1 drivers
v0x85a2c0 .array "mem", 0 4, 7 0;
v0x85a340_0 .alias "read_data", 7 0, v0x85a610_0;
v0x85a3f0_0 .net "write_data", 7 0, v0x85a6c0_0; 1 drivers
v0x85a490_0 .net "write_enable", 0 0, v0x85a770_0; 1 drivers
E_0x813c90 .event edge, v0x85a490_0;
L_0x85a850 .array/port v0x85a2c0, v0x85a570_0;
    .scope S_0x813690;
T_0 ;
    %vpi_call 2 30 "$readmemh", "datamemory.list", v0x813870;
    %end;
    .thread T_0;
    .scope S_0x849650;
T_1 ;
    %wait E_0x813c90;
    %load/v 8, v0x85a490_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.0, 4;
    %load/v 8, v0x85a3f0_0, 8;
    %ix/getv 3, v0x85a220_0;
    %jmp/1 t_0, 4;
    %ix/load 0, 8, 0; word width
    %ix/load 1, 0, 0; part off
    %assign/av v0x85a2c0, 0, 8;
t_0 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x813780;
T_2 ;
    %vpi_call 2 49 "$display", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x85a570_0, v0x85a6c0_0, v0x85a770_0, v0x85a610_0;
    %set/v v0x85a770_0, 0, 1;
    %set/v v0x85a570_0, 0, 6;
    %movi 8, 1, 8;
    %set/v v0x85a6c0_0, 8, 8;
    %vpi_call 2 53 "$monitor", "data_address=%b, write_data=%b, write_enable=%b, read_data=%b", v0x85a570_0, v0x85a6c0_0, v0x85a770_0, v0x85a610_0;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "data_mem.v";
